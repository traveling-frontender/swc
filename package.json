{
  "name": "swc",
  "version": "0.0.0",
  "private": true,
  "author": "kaputin <kaputinea@gmail.com>",
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.vue, --ignore-path .gitignore --fix src",
    "kill": "lsof -t -i tcp:3000,8080 | xargs kill -9",
    "test": "vitest run",
    "version-update": "yarn config set version-git-tag false && yarn version --new-version $VERSION && git push --tags &&  git add . && git commit -m \"version update\" && git push && echo \"Successfully released version: $VERSION\"",
    "post-release": "git checkout master && git push && git push --tags && git checkout develop && git push"
  },
  "dependencies": {
    "@types/vue-the-mask": "^0.11.5",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "swiper": "^11.1.3",
    "vue": "^3.4.15",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5",
    "vue-select": "^4.0.0-beta.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@types/lodash": "^4.14.198",
    "@types/vue-select": "^3.16.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-legacy": "^5.3.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.1.5",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "terser": "^5.19.4",
    "typescript": "~5.2.2",
    "vite": "^5.0.12",
    "vite-plugin-eslint": "^1.8.1",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.3.0",
    "vue-the-mask": "^0.11.1",
    "vue-tsc": "^1.8.27",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "yarn lint"
    ]
  },
  "packageManager": "yarn@4.1.0"
}
